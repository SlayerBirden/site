{
    "name": "maketok/site",
    "description": "Maketok site framework",
    "license": "MIT",
    "keywords": [
        "site",
        "maketok",
        "php",
        "cms",
        "framework"
    ],
    "authors": [
        {
            "name": "Oleg Kulik",
            "email": "olegkulik1985@gmail.com"
        }
    ],
    "repositories": [
        {
            "type": "composer",
            "url": "https://packages.zendframework.com/"
        }
    ],
    "homepage": "http://maketok.com/",
    "require": {
        "php": ">=5.4.0",
        "zendframework/zend-db": "2.*",
        "zendframework/zend-stdlib": "2.*",
        "zendframework/zend-uri": "2.*",
        "zendframework/zend-eventmanager": "2.*",
        "monolog/monolog": "~1.0",
        "symfony/http-foundation": "2.*",
        "symfony/dependency-injection": "2.*",
        "symfony/config": "2.*",
        "symfony/form": "2.*",
        "symfony/twig-bridge": "2.*",
        "symfony/validator": "2.*",
        "symfony/expression-language": "2.*",
        "symfony/security": "2.*",
        "twig/twig": "~1",
        "twig/extensions": "1.*",
        "erusev/parsedown-extra": "~0.2",
        "aptoma/twig-markdown": "~0.2"
    },
    "require-dev": {
        "phpunit/PHPUnit": "4.*",
        "mikey179/vfsStream": "~1.0"
    },
    "autoload": {
        "psr-0": {
            "Maketok\\": "lib/",
            "admin\\": "src/",
            "base\\": "src/",
            "modules\\": "./",
            "tests\\": "./"
        },
        "psr-4": {
            "Zend\\Db\\": "lib/Zend/Db",
            "Symfony\\Bundle\\": "lib/Symfony/Bundle",
            "Symfony\\Component\\Form\\": "lib/Symfony/Component/Form"
        }
    }
}

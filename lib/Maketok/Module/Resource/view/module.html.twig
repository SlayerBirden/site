{% extends "base-manager.html.twig" %}
{% form_theme form 'bootstrap_3_layout.html.twig' %}
{% block head %}
    {{ parent() }}
{% endblock %}
{% block content %}
    <form method="post" action="{{ current_url }}">
        {{ form_widget(form) }}
        <button class="btn btn-default" type="reset">{% trans %}Reset{% endtrans %}</button>
        <button class="btn btn-primary" type="submit">{% trans %}Submit{% endtrans %}</button>
        <a href="{{ base_url }}/modules">{% trans %}Back{% endtrans %}</a>
    </form>

{% endblock %}
